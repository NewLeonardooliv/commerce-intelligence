{
  "name": "commerce-intelligence",
  "version": "1.0.0",
  "description": "Intelligent data analysis service with AI agents",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/app.ts",
    "start": "bun src/app.ts",
    "build": "bun build src/app.ts --outdir dist --target bun",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "type-check": "bun tsc --noEmit",
    "format": "bunx prettier --write \"src/**/*.ts\"",
    "format:check": "bunx prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/eden": "^1.1.5",
    "@elysiajs/swagger": "^1.1.5",
    "elysia": "^1.1.30",
    "openai": "^6.16.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.13",
    "typescript": "^5.7.2",
    "prettier": "^3.4.2"
  },
  "keywords": [
    "elysia",
    "bun",
    "typescript",
    "ai",
    "analytics",
    "api",
    "rest"
  ],
  "author": "",
  "license": "MIT"
}
